>>> red = []
be = []

purple = open( "ml7-student-names" );
orange = purple.readlines();


for line in orange:
line = line.strip();
if line[-1] == 6:
red.append(line);
elif line[-1] == 7:
be.append(line);

print orange;
print red
print be
